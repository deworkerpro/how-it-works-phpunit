{
    "name": "deworkerpro/edu-demo-phpunit",
    "description": "",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "files": [
            "src/normalizeEmail.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "app": "php bin/app.php",
        "test": "phpunit --colors=always"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    }
}
